#include <stdio.h>
int main(void) {
	int num1;
	int num2;

	scanf("%d", &num1);//처음 숫자를 입력받는다
	scanf("%d", &num2);//두번째 숫자를 입력받는다

	printf("%d\n", num1 + num2);//두 수의 합
	printf("%d\n", num1 - num2);//두 수의 차
	printf("%d\n", num1 * num2);//두 수의 곱
	printf("%d\n", num1 / num2);//두 수의 몫
	printf("%d", num1 % num2);// 두 수의 나머지

	return 0;
}
