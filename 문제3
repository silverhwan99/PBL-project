#include <stdio.h>

int main(void) {
	int x;
	int y;

	scanf("%d", &x); //x값 입력
	scanf("%d", &y); //y값 입력

	if (x > 0 && y > 0) {
		printf("1사분면"); // x 양수,y 양수 이면 1사분면 출력
	}
	else if (x < 0 && y > 0) {
		printf("2사분면"); // x 음수, y 양수 이면 2사분면 출력
	}
	else if (x < 0 && y < 0) {
		printf("3사분면"); // x 음수, x 음수 이면 3사분면 출력
	}
	else {
		printf("4사분면"); // x 양수, x 음수 이면 4사분면 출력
	}
	return 0;
}
